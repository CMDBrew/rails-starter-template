# Ignore Mac .DS_Store
.DS_Store

# Ignore local configs.
/.bundle
/.vscode

# Ignore Local Variables
.env
.keep

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore byebug history
.byebug_history

# Ignore rbenv files
.ruby-version

# Ignore local uploads
public/uploads/*

# Ignore simplecov reports
/coverage

# Ignore ERD
erd.pdf
